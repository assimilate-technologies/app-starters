{
  "name": "node-typescript-app-starter",
  "version": "0.0.1",
  "description": "App starter for Node with TypeScript",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config build/index.js",
    "deploy-prod": "npm install && npm run build && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assimilate-technologies/app-starters.git"
  },
  "keywords": [
    "Node",
    "TypeScript",
    "AppStarter"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assimilate-technologies/app-starters/issues"
  },
  "homepage": "https://github.com/assimilate-technologies/app-starters#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "dotenv": "^16.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
